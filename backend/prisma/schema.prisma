// Prisma schema file
// Documentação: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model user {
  id      String  @id @default(auto()) @map("_id") @db.ObjectId
  email   String  @unique
  name    String
  age     Int     
}
